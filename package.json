{
  "name": "@cloudquery/setup-cloudquery",
  "version": "1.0.0",
  "description": "Setup CloudQuery CLI in a GitHub action environment",
  "main": "dist/index.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "ncc build src/main.js -o dist",
    "format": "prettier --write src/**/*.js",
    "format:ci": "prettier --check src/**/*.js",
    "lint": "eslint 'src/**/*.js'",
    "start": "node src/main.js"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cloudquery/setup-cloudquery.git"
  },
  "type": "module",
  "author": "Netlify Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cloudquery/setup-cloudquery/issues"
  },
  "homepage": "https://github.com/cloudquery/setup-cloudquery#readme",
  "dependencies": {
    "@actions/core": "^1.7.0",
    "@actions/github": "^5.0.0",
    "chalk": "^5.0.0",
    "execa": "^6.0.0",
    "ora": "^6.0.0",
    "pg": "^8.7.3",
    "semver": "^7.3.7"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.0",
    "@commitlint/config-conventional": "^16.0.0",
    "@vercel/ncc": "^0.33.4",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "prettier": "^2.0.5"
  }
}
